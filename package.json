    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "dotenv-webpack": "^6.0.4",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "mini-css-extract-plugin": "^1.6.2",
